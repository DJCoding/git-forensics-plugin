<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:f="/lib/form">

        <f:entry title="${%title.maxCommits}" description="${%description.maxCommits}" field="maxCommits">
            <f:number default="100"/>
        </f:entry>

        <f:entry title="${%title.referenceJob}" field="referenceJobName" description="${%description.referenceJob}">
            <f:combobox/>
        </f:entry>

        <f:entry title="${%title.skipUnknownCommits}" description="${%description.skipUnknownCommits}" field="skipUnknownCommits">
            <f:checkbox />
        </f:entry>

        <f:entry title="${%title.newestBuildIfNotFound}" description="${%description.newestBuildIfNotFound}" field="newestBuildIfNotFound">
            <f:checkbox />
        </f:entry>
</j:jelly>